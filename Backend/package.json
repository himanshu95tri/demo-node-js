{
  "name": "y",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "dev": "nodemon ./src/index.ts",
    "generate-models": "sequelize-auto -h localhost -d triCreator -u postgres -p 5432 -x 1234567 -e postgres -o ./models --typescript",
    "migrate:local": "sequelize --config src/config/config.json --migrations-path src/migrations db:migrate",
    "migrate:development": "NODE_ENV=development sequelize --config src/config/config.json --migrations-path src/migrations db:migrate",
    "migrate:production": "NODE_ENV=production sequelize --config src/config/config.json --migrations-path src/migrations db:migrate",
    "migrate:undo": "sequelize --config src/config/config.json --migrations-path src/migrations db:migrate:undo",
    "migrate:undo:all": "sequelize --config src/config/config.json --migrations-path src/migrations db:migrate:undo:all",
    "start-development": "NODE_ENV=development pm2 start ./dist/index.js --name=triCreator",
    "build": "tsc"
  },
  "author": "Himanshu Patel",
  "license": "ISC",
